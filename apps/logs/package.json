{
  "name": "logs",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "fastify start dist/app.js",
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start dist/app.js --port 3001",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "docker:build": "cd ../.. && docker build -t remix-gospel-stack-logs -f ./apps/logs/Dockerfile .",
    "env:pull": "pnpm dlx infisical pull dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/postgres": "^5.1.0",
    "@fastify/sensible": "^5.0.0",
    "cuid": "^2.1.8",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.5.1",
    "fastify-plugin": "^4.0.0",
    "fastify-type-provider-zod": "^1.1.7",
    "pg": "^8.8.0",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/pg": "^8.6.5",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "^4.3.0",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  }
}
