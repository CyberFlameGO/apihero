    "page-with": "^0.6.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "tsup": "^6.2.3",
    "tsx": "^3.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.24.4",
    "webpack": "^5.74.0"
