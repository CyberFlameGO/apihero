        "run",
        "${relativeFile}",
        "-r",
        "${workspaceFolder}/packages/interceptors-js",
        "-c",
        "${workspaceFolder}/packages/interceptors-js/vitest.browser.config.ts",
        "--run"
      ],
      "smartStep": true,
      "console": "integratedTerminal"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: dev",
      "request": "launch",
      "command": "yarn run dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
