{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "internal-logs/*": ["../../packages/internal-logs/src/*"],
      "internal-logs": ["../../packages/internal-logs/src/index"]
    }
  },
  "include": ["src/**/*", "bindings.d.ts"]
}
